{
  "name": "js-sc-agents",
  "version": "0.0.1",
  "description": "Wrapper of sc-agents implemented in js",
  "main": "index.js",
  "scripts": {
    "start": "webpack --watch",
    "test": "jest --notify --watch"
  },
  "author": "rizhi-kote",
  "license": "ISC",
  "dependencies": {
    "@types/ramda": "^0.25.24",
    "@types/jest": "^22.2.3",
    "jquery": "^3.3.1",
    "ramda": "^0.25.0",
    "underscore": "^1.8.3",
    "validate-arguments": "0.0.8"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.6.1",
    "jest": "^22.4.3",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^22.4.4",
    "ts-loader": "^3.2.0",
    "typescript": "^2.8.3",
    "webpack": "^3.11.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "notify": true,
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".*spec\\.(ts|tsx)$"
  }
}
